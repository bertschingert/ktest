#!/usr/bin/env bash

. $(dirname $(readlink -e "${BASH_SOURCE[0]}"))/../xfstests.sh

require-kernel-config EXT4_FS
require-kernel-config EXT4_FS_POSIX_ACL
require-kernel-config EXT4_DEBUG
require-kernel-config QUOTA

export FSTYP=ext4
export MKFS_OPTIONS=

run_tests()
{
    run_xfstests "$@"
}

main "$@"
